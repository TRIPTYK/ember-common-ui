<div class={{unless @classless 'tpk-select'}} ...attributes>
  {{yield
    (hash
      Label=(component
        'tpk-select/label' guid=this.guid classless=@classless label=@label
      )
      Container=(component
        'tpk-select/container'
        isOpen=this.isOpen
        selectElement=this.selectElement
        selectButtonClicked=this.selectButtonClicked
        selected=this.selected
        classless=@classless
        multiple=@multiple
        label=@label
        guid=this.guid
        options=@options
        selectedIndex=this.selectedIndex
      )
    )
  }}
</div>