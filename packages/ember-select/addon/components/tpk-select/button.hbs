<div
  aria-controls={{@optionListId}}
  aria-expanded='{{@isOpen}}'
  aria-labelledby={{@labelId}}
  optionListId='listbox'
  role='combobox'
  tabindex='0'
  {{on 'click' @onSelectButtonClick}}
  class={{unless @classless 'tpk-select-button'}}
  {{did-insert @registerControllerDiv}}
  ...attributes
>
  {{yield @selected}}
</div>