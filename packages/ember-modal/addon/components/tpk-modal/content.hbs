<div
  data-test-tpk-modal
  class='tpk-modal-content'
  ...attributes
>
  {{#if (has-block-params)}}
    {{yield
      (hash
        Head=(component 'tpk-modal/content/head' onClose=@onClose title=@title)
      )
    }}
  {{else}}
    <TpkModal::Content::Head @onClose={{@onClose}} @title={{@title}} />
    {{yield}}
  {{/if}}
</div>  