    "rsvp": "^4.8.5",
    "type-fest": "^4.24.0",
    "typescript": "^5.5.4",
    "webpack": "^5.95.0",
    "@triptyk/ember-input": "3.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
