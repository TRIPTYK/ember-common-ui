{
  "name": "doc-app",
  "version": "0.0.0",
  "private": true,
  "description": "UI & Inputs for TRIPTYK Ember projects",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "url": "git@github.com:TRIPTYK/ember-common-ui.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
